{
  "manifest_version": 3,
  "name": "NewTab",
  "version": "0.01",
  "minimum_chrome_version": "37",
  "permissions": [
          "storage",
          "tabs",
          "bookmarks",
          "sessions",
          "topSites",
          "history"
        ],
	"chrome_url_overrides" : {
	  "newtab": "start.html"
	}
}