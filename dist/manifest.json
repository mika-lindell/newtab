{
  "manifest_version": 2,
  "name": "TABU – The New Tab Page",
  "short_name": "TABU",
  "description": "Less is more – we focus on what you need to do to get things done – the news or weather isn't a priority!",
  "version": "0.5.0.9",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "minimum_chrome_version": "37",
  "permissions": [
          "storage",
          "tabs",
          "bookmarks",
          "sessions",
          "topSites",
          "history"
        ],
	"chrome_url_overrides" : {
	  "newtab": "start.html"
	}
}