!!! 5
%html
	%head
		%title
		%link(href="styles/app.css" rel="stylesheet")
		%link(href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
	%body(data-app="newTab" id="app")
		.container
			.row
				.col.s12
					#most-visited
						%h2 Top
			.row
				.col.s12.center
					%button.btn-large#view-bookmarks
						View Bookmarks
					%button.btn-large#view-history
						View History
					%button.btn-large#go-incognito
						%i.material-icons.right
							open_in_new
						Go Incognito
			.row
				.col.s4
					#recent-bookmarks
						%h2 Marks
				.col.s4
					#recently-closed
						%h2 Closed
				.col.s4
					#other-devices
						%h2 Devices

		%script(src="./scripts/app.js")
	